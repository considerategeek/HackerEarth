#include <stdio.h>
#include<ctype.h>
#include<string.h>

int main()
{
    int test,a,b,size,i;
    scanf("%d",&test);
    for (i=0;i<test;i++)
    {
       scanf("%d",&size);
       if (size%2==0)
        printf("%d %d\n",size/2,size/2);
       else
        printf("%d %d\n",size/2,(size/2+1));

    }


}
