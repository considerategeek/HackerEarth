#include <stdio.h>
#include<ctype.h>
#include<string.h>

int main()
{
     int num[10],i,j,count=0;


     for (i=0;i<3;i++)
     {
         scanf("%d",&num[i]);

     }
     for (j=num[0];j<(num[1]+1);j++)

     {
         if(j%num[2]==0)
     {
         count+=1;
     }

     }
     printf("%d",count);

}
