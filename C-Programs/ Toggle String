#include <stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
      int i;
      char s[110];
      scanf("%[^\n]",s);
      for(i=0;i<strlen(s);i++)
      {
        if(s[i]>=97 && s[i]<=122)
        {
              s[i]=s[i]-32;
        }
        else
        {
            s[i]=s[i]+32;
        }
      }

printf("%s",s);
}
